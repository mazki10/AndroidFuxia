<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
             android:layout_height="match_parent">

    <TextView
        android:id="@+id/tvPartner"
        android:layout_width="match_parent"
        android:layout_height="36dp"
        android:text="@string/modPartnerTitulo"
        android:textAlignment="center"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/scrollView2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_wrapBehaviorInParent="horizontal_only" />

    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="382dp"
        android:layout_height="423dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.448"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.357">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:fontFamily="monospace"
                android:paddingRight="50dp"
                android:text="@string/nombre"
                android:textAlignment="textStart"
                android:textColor="@color/gray"
                android:textStyle="bold"
                tools:layout_editor_absoluteX="32dp"
                tools:layout_editor_absoluteY="91dp" />

            <EditText
                android:id="@+id/editTextText3"
                android:layout_width="195dp"
                android:layout_height="50dp"
                android:ems="10"
                android:inputType="text"
                android:paddingRight="50dp"
                tools:layout_editor_absoluteX="159dp"
                tools:layout_editor_absoluteY="76dp" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="26dp"
                android:fontFamily="monospace"
                android:paddingRight="50dp"
                android:text="@string/CIF"
                android:textAlignment="textStart"
                android:textColor="@color/gray"
                android:textStyle="bold"
                tools:layout_editor_absoluteX="32dp"
                tools:layout_editor_absoluteY="144dp" />

            <EditText
                android:id="@+id/editTextTextPostalAddress2"
                android:layout_width="195dp"
                android:layout_height="50dp"
                android:ems="10"
                android:inputType="textPostalAddress"
                android:paddingRight="50dp"
                tools:layout_editor_absoluteX="159dp"
                tools:layout_editor_absoluteY="126dp" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="wrap_content"
                android:layout_height="26dp"
                android:fontFamily="monospace"
                android:paddingRight="50dp"
                android:text="@string/direccion"
                android:textAlignment="textStart"
                android:textColor="@color/gray"
                android:textStyle="bold"
                tools:layout_editor_absoluteX="32dp"
                tools:layout_editor_absoluteY="214dp" />

            <EditText
                android:id="@+id/editTextTextPostalAddress1"
                android:layout_width="195dp"
                android:layout_height="50dp"
                android:ems="10"
                android:inputType="textPostalAddress"
                android:paddingRight="50dp"
                tools:layout_editor_absoluteX="159dp"
                tools:layout_editor_absoluteY="176dp" />

            <TextView
                android:id="@+id/textView14"
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:fontFamily="monospace"
                android:paddingRight="50dp"
                android:text="@string/tlf1"
                android:textAlignment="textStart"
                android:textColor="@color/gray"
                android:textStyle="bold"
                tools:layout_editor_absoluteX="32dp"
                tools:layout_editor_absoluteY="281dp" />

            <EditText
                android:id="@+id/editTextPhone"
                android:layout_width="195dp"
                android:layout_height="50dp"
                android:ems="10"
                android:inputType="phone"
                android:paddingRight="50dp"
                tools:layout_editor_absoluteX="159dp"
                tools:layout_editor_absoluteY="226dp" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:fontFamily="monospace"
                android:paddingRight="50dp"
                android:text="@string/email"
                android:textAlignment="textStart"
                android:textColor="@color/gray"
                tools:layout_editor_absoluteX="32dp"
                tools:layout_editor_absoluteY="334dp" />

            <EditText
                android:id="@+id/editTextTextEmailAddress"
                android:layout_width="195dp"
                android:layout_height="50dp"
                android:ems="10"
                android:inputType="textEmailAddress"
                android:paddingRight="50dp"
                tools:layout_editor_absoluteX="159dp"
                tools:layout_editor_absoluteY="276dp" />

            <TextView
                android:id="@+id/textView13"
                android:layout_width="wrap_content"
                android:layout_height="26dp"
                android:paddingRight="50dp"
                android:text="@string/zona"
                android:textColor="@color/gray" />

            <EditText
                android:id="@+id/editTextNumber2"
                android:layout_width="192dp"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="number"
                android:paddingRight="50dp" />
        </LinearLayout>
    </ScrollView>

    <Button
        android:id="@+id/btguardar"
        android:layout_width="139dp"
        android:layout_height="43dp"
        android:gravity="center"
        android:text="@string/guardar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.121"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/scrollView2"
        app:layout_constraintVertical_bias="0.393" />

    <Button
        android:id="@+id/btguardar1"
        android:layout_width="139dp"
        android:layout_height="43dp"
        android:gravity="center"
        android:text="@string/Partnerborrar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.878"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/scrollView2"
        app:layout_constraintVertical_bias="0.393" />

</androidx.constraintlayout.widget.ConstraintLayout>