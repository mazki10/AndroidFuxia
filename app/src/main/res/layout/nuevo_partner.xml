<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center_horizontal"
    android:orientation="vertical"
    android:paddingTop="10dp">

    <TextView
        android:id="@+id/textView11"
        android:layout_width="match_parent"
        android:layout_height="36dp"
        android:text="@string/nuevo_partner"
        android:textAlignment="center"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_wrapBehaviorInParent="horizontal_only"
        tools:layout_editor_absoluteX="73dp"
        tools:layout_editor_absoluteY="16dp" />

    <ScrollView
        android:layout_width="382dp"
        android:layout_height="423dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:fontFamily="monospace"
                android:paddingRight="50dp"
                android:text="@string/nombre"
                android:textAlignment="textStart"
                android:textColor="@color/gray"
                android:textStyle="bold"
                tools:layout_editor_absoluteX="32dp"
                tools:layout_editor_absoluteY="91dp" />

            <EditText
                android:id="@+id/editTextText3"
                android:layout_width="195dp"
                android:layout_height="50dp"
                android:ems="10"
                android:inputType="text"
                android:paddingRight="50dp"
                tools:layout_editor_absoluteX="159dp"
                tools:layout_editor_absoluteY="76dp" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="26dp"
                android:fontFamily="monospace"
                android:paddingRight="50dp"
                android:text="@string/CIF"
                android:textAlignment="textStart"
                android:textColor="@color/gray"
                android:textStyle="bold"
                tools:layout_editor_absoluteX="32dp"
                tools:layout_editor_absoluteY="144dp" />

            <EditText
                android:id="@+id/editTextTextPostalAddress2"
                android:layout_width="195dp"
                android:layout_height="50dp"
                android:ems="10"
                android:inputType="textPostalAddress"
                android:paddingRight="50dp"
                tools:layout_editor_absoluteX="159dp"
                tools:layout_editor_absoluteY="126dp" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="wrap_content"
                android:layout_height="26dp"
                android:fontFamily="monospace"
                android:paddingRight="50dp"
                android:text="@string/direccion"
                android:textAlignment="textStart"
                android:textColor="@color/gray"
                android:textStyle="bold"
                tools:layout_editor_absoluteX="32dp"
                tools:layout_editor_absoluteY="214dp" />

            <EditText
                android:id="@+id/editTextTextPostalAddress1"
                android:layout_width="195dp"
                android:layout_height="50dp"
                android:ems="10"
                android:inputType="textPostalAddress"
                android:paddingRight="50dp"
                tools:layout_editor_absoluteX="159dp"
                tools:layout_editor_absoluteY="176dp" />

            <TextView
                android:id="@+id/textView14"
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:fontFamily="monospace"
                android:paddingRight="50dp"
                android:text="@string/tlf1"
                android:textAlignment="textStart"
                android:textColor="@color/gray"
                android:textStyle="bold"
                tools:layout_editor_absoluteX="32dp"
                tools:layout_editor_absoluteY="281dp" />

            <EditText
                android:id="@+id/editTextPhone"
                android:layout_width="195dp"
                android:layout_height="50dp"
                android:ems="10"
                android:inputType="phone"
                android:paddingRight="50dp"
                tools:layout_editor_absoluteX="159dp"
                tools:layout_editor_absoluteY="226dp" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:fontFamily="monospace"
                android:paddingRight="50dp"
                android:text="@string/email"
                android:textAlignment="textStart"
                android:textColor="@color/gray"
                tools:layout_editor_absoluteX="32dp"
                tools:layout_editor_absoluteY="334dp" />

            <EditText
                android:id="@+id/editTextTextEmailAddress"
                android:layout_width="195dp"
                android:layout_height="50dp"
                android:ems="10"
                android:inputType="textEmailAddress"
                android:paddingRight="50dp"
                tools:layout_editor_absoluteX="159dp"
                tools:layout_editor_absoluteY="276dp" />

            <TextView
                android:id="@+id/textView10"
                android:layout_width="wrap_content"
                android:layout_height="26dp"
                android:fontFamily="monospace"
                android:paddingRight="50dp"
                android:text="@string/comercial"
                android:textAlignment="textStart"
                android:textColor="@color/gray"
                android:textStyle="bold"
                tools:layout_editor_absoluteX="32dp"
                tools:layout_editor_absoluteY="401dp" />

            <EditText
                android:id="@+id/editTextText5"
                android:layout_width="192dp"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="text"
                android:paddingRight="50dp" />

            <TextView
                android:id="@+id/textView13"
                android:layout_width="wrap_content"
                android:layout_height="26dp"
                android:paddingRight="50dp"
                android:text="TextView"
                tools:text="@string/zona" />

            <EditText
                android:id="@+id/editTextNumber2"
                android:layout_width="192dp"
                android:layout_height="wrap_content"
                android:ems="10"
                android:hint="@string/idzona"
                android:inputType="number"
                android:paddingRight="50dp" />
        </LinearLayout>
    </ScrollView>

    <Button
        android:id="@+id/btguardar"
        android:layout_width="254dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:gravity="center"
        android:text="@string/guardar"
        tools:layout_editor_absoluteX="152dp"
        tools:layout_editor_absoluteY="580dp" />

</LinearLayout>
